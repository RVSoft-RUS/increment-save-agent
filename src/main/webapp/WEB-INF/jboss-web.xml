<?xml version="1.0" encoding="UTF-8"?>
<jboss-web>

    <resource-ref>
        <res-ref-name>jdbc/Staging</res-ref-name>
        <res-type>javax.sql.DataSource</res-type>
        <jndi-name>java:/env/jdbc/Staging</jndi-name>
    </resource-ref>

    <resource-env-ref>
        <resource-env-ref-name>kafka/bootstrapServers</resource-env-ref-name>
        <jndi-name>java:/env/kafka/bootstrapServers</jndi-name>
    </resource-env-ref>

    <resource-env-ref>
        <resource-env-ref-name>kafka/appId</resource-env-ref-name>
        <jndi-name>java:/env/kafka/appId</jndi-name>
    </resource-env-ref>

    <resource-env-ref>
        <resource-env-ref-name>kafka/topicName</resource-env-ref-name>
        <jndi-name>java:/env/kafka/topicName</jndi-name>
    </resource-env-ref>

    <resource-env-ref>
        <resource-env-ref-name>kafka/modeId</resource-env-ref-name>
        <jndi-name>java:/env/kafka/modeId</jndi-name>
    </resource-env-ref>

    <resource-env-ref>
        <resource-env-ref-name>increment/cronExpression</resource-env-ref-name>
        <jndi-name>java:/env/increment/cronExpression</jndi-name>
    </resource-env-ref>

    <resource-env-ref>
        <resource-env-ref-name>increment/mode/manual</resource-env-ref-name>
        <jndi-name>java:/env/increment/mode/manual</jndi-name>
    </resource-env-ref>

    <resource-env-ref>
        <resource-env-ref-name>logs/filePath</resource-env-ref-name>
        <jndi-name>java:/env/logs/filePath</jndi-name>
    </resource-env-ref>

    <resource-env-ref>
        <resource-env-ref-name>logs/arhSize</resource-env-ref-name>
        <jndi-name>java:/env/logs/arhSize</jndi-name>
    </resource-env-ref>

    <resource-env-ref>
        <resource-env-ref-name>increment/pagination/pageSize</resource-env-ref-name>
        <jndi-name>java:/env/increment/pagination/pageSize</jndi-name>
    </resource-env-ref>

</jboss-web>